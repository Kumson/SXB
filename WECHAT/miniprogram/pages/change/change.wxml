
<view class="container">


<view class="form_blank">

</view>
<block wx:for="{{queryResult}}" >

<view class="form_group">
        <text class="ar_cont_h2">单号:</text>
        <input type="number" placeholder="请输入快递单号 " placeholder-class="placeholder_style" name="data_num" value="{{item.postnum}}" focus="{{focus}}" bindinput='confirm' bindblur='userNumInput'/>
    </view>

 <!--输入时间-->
 <view class="ar_cont">
        <text class="ar_cont_h1">取件时间:</text>
        <view class="ar_date_time">
            <view class="ar_date">
                
                <picker mode="date" placeholder="选择日期" value="{date}}" start="2018-11-14" end="2019-11-14" bindchange="bindDateChange" >
                    <view>
                        {{date}}
                    </view>
                </picker>
            </view>

            <view class="ar_time">
                
                <picker mode="time" placeholder="选择时间" value="{{time}}" start="01:00" end="24:00" bindchange="bindTimeChange">
                    <view>
                        {{time}}
                    </view>
                </picker>
            </view>
        </view>
</view>






<view class="form_group">
        <text class="ar_cont_h3">电话:</text>
        <input type="number" placeholder="请输入取件人电话 " placeholder-class="placeholder_style" name="data_phone" value="{{item.phonenum}}"    maxlength='11' bindinput='confirm' bindblur='userPhoneNumInput'/>
    </view>


    <view class="form_group">
        <text class="ar_cont_h4">姓名:</text>
        <input type="text" placeholder="请输入收件人姓名" placeholder-class="placeholder_style" name="data_name" value="{{item.name}}" focus="{{focus}}" bindinput='confirm' bindblur='userNameInput' />
    </view>

    <view class="form_group">
         <text class="ar_cont_h5">地址:</text>
        <input type="text" placeholder="请输入预约取件地点 " placeholder-class="placeholder_style" name="data_address" value="{{item.address}}" focus="{{focus}}" bindinput='confirm' bindblur='userAddressInput'/>
    </view>


<view  class='tapLogin' bindtap='toFrontpage' >
<button bindtap="comfirm" >提交</button>
 
</view>


 </block>

</view>
